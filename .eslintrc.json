{
    "env": {
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "extends": ["airbnb-base"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": "off",
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-bitwise": "off",
        "max-len": ["error", { "code": 160 }]
    }
}
